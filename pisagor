a = 1
b = 1
liste = [i ** 2 for i in range(1, 100) ]
for i in liste:
    print(i)
while a < 100:
    
    while b < 100:
        
        toplam = (a ** 2) + (b ** 2)
        
        for x in liste:
            if toplam == x:
                print("a:", a, "b:", b, "c:",(x**0.5))
        b += 1
    a += 1
            
